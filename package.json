{
    "name": "kodama-ui",
    "version": "0.1.1",
    "description": "Kodama UI is a Vue 3 component library that provides a set of components & funcionality to build your application.",
    "homepage": "https://50l3r.github.io/kodama-ui",
    "keywords": [
        "Vue",
        "Vue3",
        "Components",
        "Web"
    ],
    "license": "MIT",
    "author": {
        "name": "Calltek",
        "email": "sistemas@calltek.es",
        "url": "https://calltek.es"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/50l3r/kodama-ui.git"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/kodama.umd.js",
    "module": "./dist/kodama.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/kodama.es.js",
            "require": "./dist/kodama.umd.js"
        },
        "./style": {
            "import": "./dist/style.css",
            "require": "./dist/style.css"
        }
    },
    "scripts": {
        "build": "npm run build:project && npm run build:storybook",
        "storybook": "start-storybook -p 6006",
        "build:storybook": "export NODE_OPTIONS=--openssl-legacy-provider && build-storybook -o docs-build",
        "build:project": "vue-tsc --noEmit && vite build",
        "deploy:storybook": "gh-pages -d storybook-static"
    },
    "dependencies": {
        "@vuelidate/core": "^2.0.0",
        "@vuelidate/validators": "^2.0.0",
        "@vuepic/vue-datepicker": "^3.4.7",
        "animate.css": "^4.1.1",
        "chance": "^1.1.8",
        "dayjs": "^1.11.7",
        "flowbite": "^1.6.3",
        "lodash.clonedeep": "^4.5.0",
        "medium-zoom": "^1.0.6",
        "object-path": "^0.11.8",
        "pinia": "^2.0.33",
        "pinia-plugin-persistedstate": "^2.1.1",
        "vue": "^3.2.37",
        "vue-loading-overlay": "^5.0.3",
        "vue-router": "^4.1.4",
        "vue3-toastify": "^0.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.19.1",
        "@storybook/addon-actions": "^6.5.12",
        "@storybook/addon-essentials": "^6.5.12",
        "@storybook/addon-interactions": "^6.5.12",
        "@storybook/addon-links": "^6.5.12",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/builder-vite": "^0.2.2",
        "@storybook/testing-library": "^0.0.13",
        "@storybook/vue3": "^6.5.12",
        "@types/chance": "^1.1.3",
        "@types/lodash.clonedeep": "^4.5.7",
        "@types/object-path": "^0.11.1",
        "@types/react": "file:stub/types__react",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "@vitejs/plugin-vue": "^3.0.3",
        "@vue/eslint-config-typescript": "^10.0.0",
        "autoprefixer": "^10.4.13",
        "babel-loader": "^8.2.5",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-storybook": "^0.6.4",
        "eslint-plugin-vue": "^8.5.0",
        "gh-pages": "^5.0.0",
        "postcss": "^8.4.20",
        "prettier": "2.6.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.55.0",
        "sass-loader": "^10.1.1",
        "storybook-dark-mode": "^1.1.2",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.6.4",
        "vite": "^3.0.7",
        "vite-plugin-dts": "^1.4.1",
        "vite-plugin-pwa": "^0.12.3",
        "vue-loader": "^16.8.3",
        "vue-tsc": "^0.39.5"
    }
}